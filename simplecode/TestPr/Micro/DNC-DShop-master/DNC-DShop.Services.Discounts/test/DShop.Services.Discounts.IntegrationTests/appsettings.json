{
  "app": {
    "name": "discounts-service"
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "discounts-service",
    "address": "docker.for.mac.localhost",
    "port": "5010",
    "pingEnabled": false,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "discounts-service",
    "requestRetries": 3
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "customers",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "discounts-service",
    "seed": false
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "dshop",
    "env": "local",
    "interval": 5
  },
  "rabbitMq": {
    "namespace": "discounts",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "restEase": {
    "loadBalancer": "",
    "services": [
      {
        "name": "orders-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5005"
      }
    ]
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  }
}