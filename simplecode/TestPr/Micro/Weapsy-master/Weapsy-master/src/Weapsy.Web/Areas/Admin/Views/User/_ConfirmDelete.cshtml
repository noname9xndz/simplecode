<div class="modal fade" id="confirm-delete" tabindex="-1" role="dialog" aria-labelledby="deleteModal" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="deleteModal">Confirm Delete</h4>
            </div>
            <div class="modal-body" data-bind="text: confirmDeleteUserMessage">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                <a id="confirmDelete" class="btn btn-danger btn-ok" data-dismiss="modal" data-bind="click: deleteUser">Delete</a>
            </div>
        </div>
    </div>
</div>